          <TableColumn fx:id="tableCompany" prefWidth="88.0" text="Company" />
            <TableColumn fx:id="tableToAddress" prefWidth="90.0" text="To Address" />
            <TableColumn fx:id="tableFromAddress" prefWidth="102.0" text="From Address" />
